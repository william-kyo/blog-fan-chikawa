# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar Time

type User {
  id: ID!  
  nickname: String!
  email: String!
  createdAt: Time!
  updatedAt: Time!
}

input NewUser {
  nickname: String!
  email: String!
}


type Mutation {
  createUser(input: NewUser!): User!
}

type Query {
  users: [User!]!
}
